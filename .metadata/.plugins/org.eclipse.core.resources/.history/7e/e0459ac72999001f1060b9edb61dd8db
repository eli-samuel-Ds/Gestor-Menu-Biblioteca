package model;

import java.util.List;

public class Autor<E> {

	//Atributos
	private String nombre; 
	private String apellido;
	private List<String> listaLibros;

	//Constructor
	public Autor(String nombreAutor, String apellidoAutor, List<String> listaLibrosAutor) {
		// TODO Auto-generated constructor stub
		this.nombre = nombreAutor;
		this.apellido = apellidoAutor;
		this.listaLibros = listaLibrosAutor;
	}
}
